<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="120.0" prefWidth="300.0" style="-fx-border-width: 4;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="memail.inbox.InboxController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Label fx:id="messageSenderLabel" text="Joseph" GridPane.halignment="LEFT">
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <Label fx:id="messageDateLabel" text="27/01/18" GridPane.columnIndex="1" GridPane.halignment="RIGHT" />
      <Label fx:id="messageSubjectLabel" text="Re: Coming out tonight?" GridPane.columnSpan="2" GridPane.rowIndex="1">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </Label>
   </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</GridPane>
